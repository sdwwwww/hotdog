";\nparent \"J1\" \"NewjotMerman1\";\ndelete \"_conSON*\";\n//select J1;Position_Group_Rame1;pickWalk -d up;rename \"NewjotMerman1\";\n\nselect $Lotall[0];rename \"NewjotMerman0\";\npointConstraint -n \"_conSOMp\" -offset 0 0 0 -weight 1 $RigChest \"NewjotMerman0\";\norientConstraint -n \"_conSOMo\" -offset 0 0 0 -weight 1 $RigChest \"NewjotMerman0\";\nparent \"NewjotMerman1\" \"NewjotMerman0\";\n\nselect J1;CurveOperation4 \"Position_Group_Rame2\";rename \"NewshoulderMerman\";\nselect -r J1 ;select -tgl J3 ;\nikHandle;rename \"ikhandleMerman\";\npointConstraint -n \"_conSOM\" -offset 0 0 0 -weight 1 \"NewForeMerman\" \"ikhandleMerman\";\n\nselect -r $RigShoulder \"NewshoulderMerman\";\norientConstraint -n \"_conSON\";\n//orientConstraint -n \"_conSON\" -offset 1 1 1 -weight 1 $RigShoulder \"NewshoulderMerman\";\nselect \"NewshoulderMerman\";\nsetAttr -lock true \".tx\";\nsetAttr -lock true \".ty\";\nsetAttr -lock true \".tz\";\nsetAttr -lock true \".sx\";\nsetAttr -lock true \".sy\";\nsetAttr -lock true \".sz\";\nsetAttr -lock true \".v\";\nbakeResults -t (`playbackOptions -q -minTime`+\":\"+`playbackOptions -q -maxTime`) -simulation true;\ndelete \"_conSON*\";\n\nselect -r \"NewshoulderMerman\" $RigShoulder;\n\t\torientConstraint -n \"_conSOM\" -mo ;\n\ngroup \"NewForeMerman\" \"NewjotMerman0\" \"ikhandleMerman\";\nprefixHierarchy;\n}fixingshoulder;" 
