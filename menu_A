            -command "global proc zidongjianbangB(){\nstring $selerigss[]=`ls-sl`;\nGetFormLocator2;string $shannn[]=`ls-sl`;autoaddaimlot;delete $shannn;\nselect $selerigss[0] locator_Ttive locator_Vtive;\naddaimlotB;LtW_point;\nselect $selerigss[0];foundconsaki;PickWalkUp;PickWalkUp;\nstring $groupkoA[]=`ls-sl`;//Niangzi\nPickWalkRight;\nstring $groupkoB[]=`ls-sl`;//TTT\nPickWalkRight;\nstring $groupkoC[]=`ls-sl`;//PVV\nselect $selerigss[1] $groupkoB $groupkoC;\nLtL_point;gaimingzi(\"prefix_\");\nselect $groupkoC[0];pickWalk -d right;\nstring $groupkoD[]=`ls-sl`;//LtLparent\nsetAttr ($groupkoB[0]+\".blendPoint1\") 0.5;\nsetAttr ($groupkoC[0]+\".blendPoint1\") 0.25;\ngroup $groupkoA $groupkoB $groupkoC $groupkoD;\nrename \"autoOgroup______1\";outlinecolor(1050);select -cl;\n}zidongjianbangB;\n\novberlapsonW;\n\n///////////////\n//1 AとBの距離（数値x）を計算\n//2 Aの位置が原点（0,0,0）から見て、Aのローカル軸のうち、最も正方向（+1に近い）と負方向（-1に近い）に向いている軸を判定\n//3 その2軸方向にロケータを作成し、数値xだけ移動させる\nglobal proc autoaddaimlot(){\nstring $sel[] = `ls -sl`;\nif (size($sel) != 2) {error(\"2つのオブジェクトを選択してください。A（回転リグ）→B（対象リグ）の順で選択してください。\");}\nstring $rigA = $sel[0];\nstring $rigB = $sel[1];\n// AとBのワールド位置\nvector $posA = `xform -q -ws -t $rigA`;\nvector $posB = `xform -q -ws -t $rigB`;\n// AとBの距離（数値x）\nfloat $distance = mag($posB - $posA)*1.0;\n// Aのワールド行列\nfloat $matrixA[16] = `xform -q -ws -m $rigA`;\n// Aのローカル軸（X, Y, Z）をワールド空間に変換\nvector $xAxis = unit(<<$matrixA[0], $matrixA[1], $matrixA[2]>>);\nvector $yAxis = unit(<<$matrixA[4], $matrixA[5], $matrixA[6]>>);\nvector $zAxis = unit(<<$matrixA[8], $matrixA[9], $matrixA[10]>>);\n// AからBへの方向ベクトル\nvector $direction = unit($posB - $posA);\n// 各軸との内積を計算\nfloat $dotX = dot($direction, $xAxis);\nfloat $dotY = dot($direction, $yAxis);\nfloat $dotZ = dot($direction, $zAxis);\n\n// 最も正方向（最大）と負方向（最小）の軸を判定\nstring $axes[3] = {\"X\", \"Y\", \"Z\"};\nfloat $dots[3] = {$dotX, $dotY, $dotZ};\n\nint $maxIndex = 0;\nint $minIndex = 0;\nfor ($i = 1; $i < 3; $i++) {\n    if ($dots[$i] > $dots[$maxIndex]) $maxIndex = $i;\n    if ($dots[$i] < $dots[$minIndex]) $minIndex = $i;\n}\n\n// 軸ベクトル取得\nvector $axesVec[3] = {$xAxis, $yAxis, $zAxis};\nvector $maxAxis = $axesVec[$maxIndex];\nvector $minAxis = $axesVec[$minIndex];\n\n// ロケータ作成\nstring $loc1Array[] = `spaceLocator -name \"locator_Ttive\"`;outlinecolor(011);\nstring $loc2Array[] = `spaceLocator -name \"locator_Vtive\"`;outlinecolor(010);\n\nstring $loc1 = $loc1Array[0];\nstring $loc2 = $loc2Array[0];\n\n// ロケータ位置計算\nvector $loc1Pos = $posA + ($maxAxis * $distance);\nvector $loc2Pos = $posA + ($minAxis * $distance);\n\n// ロケータ移動\nxform -ws -t ($loc1Pos.x) ($loc1Pos.y) ($loc1Pos.z) $loc1;\nxform -ws -t ($loc2Pos.x) ($loc2Pos.y) ($loc2Pos.z) $loc2;\n\n// 結果表示\nprint(\"距離 x = \" + $distance + \"\\n\");\nprint(\"正方向軸: \" + $axes[$maxIndex] + \" (\" + $dots[$maxIndex] + \")\\n\");\nprint(\"負方向軸: \" + $axes[$minIndex] + \" (\" + $dots[$minIndex] + \")\\n\");\n}//autoaddaimlot;" 
